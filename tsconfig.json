{
  "compilerOptions": {
    "outDir": "./dist/",
    // 与 Vue 的浏览器支持保持一致
    "target": "es5",
    // 这可以对 `this` 上的数据 property 进行更严格的推断
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "noImplicitAny": true,
  },
  "exclude": ["node_modules"],
  "include": ["./src/"]
}